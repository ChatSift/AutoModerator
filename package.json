{
	"$schema": "http://json.schemastore.org/package",
	"name": "automoderator",
	"version": "0.1.0",
	"description": "Something cool",
	"type": "module",
	"scripts": {
		"build": "pnpm recursive run build --filter @automoderator/* --filter @chatsift/*",
		"lint": "pnpm recursive run lint --filter @automoderator/* --filter @chatsift/*",
		"lint:ci": "pnpm lint --parallel --filter @automoderator/* --filter @chatsift/*",
		"test": "jest",
		"test:ci": "jest --ci --no-stack-trace --verbose",
		"docs": "typedoc"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/didinele/AutoModerator.git"
	},
	"author": "didinele",
	"bugs": {
		"url": "https://github.com/didinele/AutoModerator/issues"
	},
	"homepage": "https://github.com/didinele/AutoModerator#readme",
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/generator": "^7.16.0",
		"@babel/plugin-proposal-decorators": "^7.15.8",
		"@babel/preset-env": "^7.16.4",
		"@babel/preset-typescript": "^7.16.0",
		"@cordis/rest": "^2.0.0-next.2405c88.1643469558",
		"@prisma/client": "^3.10.0",
		"@strictsoftware/typedoc-plugin-monorepo": "^0.4.2",
		"@types/jest": "^26.0.24",
		"@typescript-eslint/eslint-plugin": "^5.9.0",
		"@typescript-eslint/parser": "^5.9.0",
		"babel-jest": "^26.6.3",
		"babel-plugin-const-enum": "^1.1.0",
		"babel-plugin-transform-typescript-metadata": "^0.3.2",
		"eslint": "^8.6.0",
		"eslint-config-marine": "^9.3.2",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-react": "^7.27.1",
		"eslint-plugin-react-hooks": "^4.3.0",
		"husky": "^4.3.8",
		"jest": "^26.6.3",
		"lint-staged": "^11.2.0",
		"prettier": "^2.5.1",
		"prettier-eslint": "^13.0.0",
		"prisma": "^3.10.0",
		"rimraf": "^3.0.2",
		"typedoc": "0.19.2",
		"typescript": "^4.5.5"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.ts": [
			"eslint --fix"
		],
		"*.tsx": [
			"eslint --fix"
		]
	}
}
