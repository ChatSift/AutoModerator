{
	"$schema": "http://json.schemastore.org/package",
	"name": "@chatsift/automoderator",
	"packageManager": "yarn@3.2.1",
	"private": true,
	"version": "0.0.0",
	"workspaces": [
		"packages/*",
		"services/*"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/didinele/AutoModerator.git"
	},
	"author": "didinele",
	"bugs": {
		"url": "https://github.com/didinele/AutoModerator/issues"
	},
	"homepage": "https://github.com/didinele/AutoModerator#readme",
	"scripts": {
		"lint": "turbo run lint && prettier --check --plugin-search-dir=. .",
		"build": "turbo run build",
		"format": "prettier --write --plugin-search-dir=. .",
		"prisma": "dotenv -e .env prisma",
		"prepare": "is-ci || husky install",
		"update": "yarn upgrade-interactive"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.7.1",
		"@commitlint/config-angular": "^17.7.0",
		"@prisma/client": "^5.2.0",
		"dotenv-cli": "^7.3.0",
		"eslint": "^8.47.0",
		"eslint-config-neon": "^0.1.54",
		"eslint-formatter-pretty": "^5.0.0",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"prettier": "^3.0.2",
		"prisma": "^5.2.0",
		"rimraf": "^5.0.1",
		"turbo": "^1.10.12",
		"typescript": "^5.1.6"
	}
}
